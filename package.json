{
    "name": "@iota-pico/api-examples-nodejs",
    "version": "0.9.9",
    "description": "IOTA Pico Framework Examples for NodeJS using TypeScript",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "build-clean": "rimraf ./dist/*",
        "build-compile": "tsc",
        "build-lint": "tslint -p ./tsconfig.json -c ./tslint.json -t verbose",
        "build": "npm-run-all build-clean build-compile build-lint",
        "dist": "npm-run-all build"
    },
    "author": "martyn@iota.eco",
    "license": "MIT",
    "bin": {
        "ipent": "bin/ipent.js"
    },
    "dependencies": {
        "@iota-pico/api": "^0.9.9",
        "@iota-pico/core": "^0.9.9",
        "@iota-pico/pal-nodejs": "^0.9.9",
        "chalk": "^2.4.1",
        "commander": "^2.16.0"
    },
    "devDependencies": {
        "@types/node": "^10.5.1",
        "npm-run-all": "^4.1.3",
        "rimraf": "^2.6.2",
        "tslint": "^5.10.0",
        "tslint-eslint-rules": "^5.3.1",
        "tslint-microsoft-contrib": "^5.0.3",
        "typescript": "^2.9.2",
        "webpack": "^4.15.0",
        "webpack-cli": "^3.0.8"
    }
}
